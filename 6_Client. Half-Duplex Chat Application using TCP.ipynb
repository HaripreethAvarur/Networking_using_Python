    "c = socket.socket(family=socket.AF_INET, type=socket.SOCK_STREAM)\n",
    "c.connect(('localhost',5000))\n",
    "\n",
    "while True:\n",
    "    msg_tos = input(\"client (you): \")\n",
    "    c.send(msg_tos.encode())\n",
    "    msg_froms = c.recv(2048)\n",
    "    print (\"server: \", msg_froms.decode())\n",
